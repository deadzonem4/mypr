{% extends "ZingCoreAdminBundle::layout.html.twig" %}
{% trans_default_domain "zing" %}
{% block body %}
    <div id="content-column" class="col-lg-12 col-md-12">
        <div class="box">
            <div class="box-title">
                {% trans %}Sitemap{% endtrans %}
                <div class="box-title-options">
                    <a class="btn btn-sm btn-primary hidden-sm hidden-xs" href="#"><i class="fa fa-filter"></i> {% trans %}UPDATE{% endtrans %}</a>
                </div><!-- /.box-title-options -->
            </div>
            <div class="box-content">
                {{ zing_debug(sitemap.getSitemap()) }}
            </div><!-- /.box-content -->
        </div><!-- /.box -->
    </div><!-- /.col-lg-12 -->
{% endblock %}
{% block javascript %}
    <script src="{{asset('bundles/zing/admin/js/jwebsidebar.js')}}"></script>
{% endblock %}